# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'VeryUsefulToolstoRemember' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VeryUsefulToolstoRemember

  target 'VeryUsefulToolstoRememberTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SnapshotTesting'
  end

end

pre_install do |installer|
  installer.analysis_result.specifications.each do |s|
    if s.name == 'SnapshotTesting'
      s.swift_version = '5.0'
    end
  end
end